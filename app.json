        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mediatedreality.moodyradiostations"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0c749b6f-29b3-44b9-a482-edcb27ed5c30"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0c749b6f-29b3-44b9-a482-edcb27ed5c30"
    },
    "plugins": [
      "expo-router"
    ]
